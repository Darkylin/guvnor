{
  "name": "guvnor",
  "description": "A node process manager that isn't spanners all the way down",
  "version": "3.5.14",
  "license": "MIT",
  "prefer-global": true,
  "homepage": "https://github.com/tableflip/guvnor",
  "repository": {
    "type": "git",
    "url": "git://github.com/tableflip/guvnor.git"
  },
  "bugs": {
    "url": "https://github.com/tableflip/guvnor/issues"
  },
  "scripts": {
    "lint": "standard",
    "test": "npm run test:before && npm run test:integration && npm run test:browser && npm run test:after:success || npm run test:after:failure",
    "test:before": "node ./test/integration/_before.js",
    "test:integration": "ava test/integration --verbose --serial",
    "test:browser": "nightwatch -c test/browser/nightwatch.json test/browser/tests",
    "test:after:success": "node ./test/integration/_after-success.js",
    "test:after:failure": "node ./test/integration/_after-failure.js",
    "test:ci": "COVERAGE=true QUIET=true npm test && nyc report --reporter=lcov",
    "test:coverage": "COVERAGE=true npm test && nyc report --reporter=html",
    "test:coverage:check": "npm run test:coverage && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "test:heap": "TRACK_HEAP=true npm test",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "docker": "npm run docker:build && npm run docker:deploy",
    "docker:build": "docker build -f Dockerfile-guvnor-base -t achingbrain/guvnor-base .",
    "docker:deploy": "docker push achingbrain/guvnor-base"
  },
